{
  "name": "driving-school-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development nodemon ./src/server.ts",
    "build": "tsc --project ./src",
    "prettier:check": "prettier --check src",
    "prettier:fix": "prettier --write src",
    "db:create:migration": "knex migrate:make",
    "db:create:seed": "knex seed:make",
    "db:run:migrate": "knex migrate:latest",
    "db:run:seed": "knex seed:run"
  },
  "bugs": {
    "url": "https://github.com/megatroom/driving-school/issues"
  },
  "dependencies": {
    "body-parse": "^0.1.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "date-fns": "^2.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "joi": "^17.4.0",
    "joi-extract-type": "^15.0.8",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.2",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "sqlite3": "^5.0.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.10",
    "@types/date-fns": "^2.6.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/knex": "^0.16.1",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.35",
    "@types/winston": "^2.4.4",
    "nodemon": "^2.0.7",
    "prettier": "2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
}
